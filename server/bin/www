#!/usr/bin/env node
/*** Debug ***/
var debug = require('debug'),
    note = debug('weB-ohnanza:note'),
    error = function(message) {
      debug('weB-ohnanza:error')(message);
      console.error(message);
    },
    log = function(message) {
      debug('weB-ohnanza:log')(message);
      console.log(message);
    };



/*** App ***/
var app = require('../app');
